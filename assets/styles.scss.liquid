  border-width: {%- if settings.heading_divider_width == 1 -%}thin{%- else -%}{{settings.heading_divider_width }}px{%- endif -%};
  border-bottom: none;
}

.heading-divider--short {
  width: 70px;
}

.heading-divider--long {
